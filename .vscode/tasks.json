{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ProjectFinder/ProjectFinder.csproj"
            ],
            "problemMatcher": "$msCompile"
        },{
            "label": "release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "--configuration",
                "Release",
                "${workspaceFolder}/ProjectFinder/ProjectFinder.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}